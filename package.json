{
  "name": "proptypes-schema",
  "version": "0.0.1",
  "description": "Generic object schema modeled on React PropTypes",
  "main": "./lib/index.js",
  "scripts": {
    "prebuild": "mkdir -p lib",
    "build": "babel src --out-dir lib",
    "dev": "babel src --watch --out-dir lib",
    "test": "mocha 'src/**/*.test.js' --require babel-register",
    "tdd": "npm test -- --reporter min --watch",
    "clean": "rm -rf lib",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "mocha": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliot-akira/proptypes-schema.git"
  }
}
