{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "comma-dangle": 0,
    "comma-spacing": [1, {"before": false, "after": true}],
    "key-spacing": [1, {"beforeColon": false, "afterColon": true}],
    "no-console": 0,
    "no-debugger": 1,
    "no-var": 1,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-cond-assign": 1,
    "no-redeclare": 1,
    "no-unexpected-multiline": 1,
    "no-unused-vars": 0,
    "no-unused-labels": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
    "object-curly-spacing": [1, "always"],
    "semi": [1, "never"],
  },
  "globals": {
  }
}
